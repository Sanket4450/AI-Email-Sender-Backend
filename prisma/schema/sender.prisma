model Sender {
  id          String   @id @default(cuid())
  displayName String
  name        String
  apiKey      String
  esp         String
  priority    Int      @default(1)
  target      Int
  sentCount   Int      @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("sender")
}
